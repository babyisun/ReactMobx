{
  "presets": [
    ["env", { "modules": false }],
    [
      "es2015",
      {
        "module": false
      }
    ],
    "react-app",
    "react",
    "stage-2"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-decorators-legacy"
      ]
    },
    "production": {
      "plugins": [
        "transform-decorators-legacy",
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
      ]
    },
    "development": {
      "plugins": [
        "transform-decorators-legacy",
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
      ]
    }
  }
}
